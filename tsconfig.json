{
    "compilerOptions": {
        "target": "esnext",
        "moduleResolution": "bundler",
        "resolveJsonModule": true,
        "allowImportingTsExtensions": true,
        "noEmit": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "exclude": ["node_modules/@google-cloud/storage", "node_modules/google-auth-library"]
}
